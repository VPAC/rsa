# GDAL location; should contain bin/gdalinfo and lib/libgdaljni.so
# NOTE: this must be set in rsa.xml too.
gdal_prefix=/usr/local

# Limit memory usage, like the -Xmx of the JVM (optional)
max_heap=1024m

# Additional classpath entries (optional).
classpath_extra=

# Options for the Java compiler for compiling filters. May be specified more
# than once.
compile_opts=-g:source,lines,vars

# Set to "false" to disable the Nailgun Java server. Equivalent to running RSA
# with the --no-daemon option.
nailgun=false

